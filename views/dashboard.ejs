<body>
    <%- include('partials/navbar')%>
    </div>
    <div class = "content">
        <div class = "hello_user">
            <h1>Hello <%=user%></h1>
        </div>
        <% if (list.length == 0) { %>
            <h2>Swipe to like some food!</h2>
        <% } else {%>
            <div class = "list_likes">
                <h2>Your Liked Foods</h2>
            </div>
            <div class = "table_list">
                    <table>
                        <tr>
                            <th>Food</th>
                            <th>Times</th>
                        </tr>
                        <tr>
                            <% list.forEach((like) => { %>
                                <td><%= like.foodName%></td>
                                <td><%=like.timesLiked%></td>
                        </tr>
                            <% }) %>
                    </table>
            </div>
        <% } %>
        
        
    </div>
    <div class = "footer"></footer>
</body>